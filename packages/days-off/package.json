{
  "name": "@marv/days-off",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "scripts": {
    "build": "PUBLIC_URL=/days-off react-scripts build",
    "check": "eslint src && flow",
    "clean": "rm -rf build",
    "fail": "echo \"oh yeah\"",
    "format": "prettier --write --single-quote --trailing-comma all --parser flow 'src/**/*.js'",
    "postinstall": "flow-typed install",
    "start": "react-scripts start",
    "test": "react-scripts test --env=jsdom"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.10"
  },
  "devDependencies": {
    "eslint": "^4.0.0",
    "flow-bin": "^0.48.0",
    "flow-typed": "^2.1.2",
    "prettier": "^1.5.2"
  }
}
